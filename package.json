{
  "name": "onboarding",
  "version": "1.0.0",
  "description": "Sample onboarding React Native App",
  "scripts": {
    "android": "react-native run-android",
    "android-clean": "cd android; ./gradlew clean; cd ..",
    "android-list": "emulator -list-avds",
    "android-nexus4": "emulator -avd Nexus_4_API_29 &",
    "android-nexus5": "emulator -avd Nexus_5X_API_29_x86 &",
    "ios": "react-native run-ios",
    "ios-8": "react-native run-ios --simulator='iPhone 8'",
    "ios-pods": "cd ios; pod install; cd ..",
    "ios-clean": "cd ios; xcodebuild clean; cd ..",
    "start": "react-native start",
    "lint": "tslint --project tsconfig.json --format verbose",
    "test": "jest",
    "postinstall": "jetify"
  },
  "dependencies": {
    "@ptomasroos/react-native-multi-slider": "^2.2.0",
    "@react-native-community/masked-view": "^0.1.7",
    "@react-navigation/native": "^5.0.8",
    "@react-navigation/stack": "^5.1.0",
    "react": "~16.9.0",
    "react-dom": "~16.9.0",
    "react-native": "~0.61.4",
    "react-native-date-picker": "^2.7.8",
    "react-native-gesture-handler": "^1.6.0",
    "react-native-reanimated": "^1.7.0",
    "react-native-safe-area-context": "^0.7.3",
    "react-native-screens": "^2.2.0",
    "react-native-unimodules": "~0.7.0",
    "react-native-web": "~0.11.7",
    "react-navigation": "^4.2.2"
  },
  "devDependencies": {
    "@babel/core": "~7.6.0",
    "@types/jest": "^25.1.4",
    "@types/ptomasroos__react-native-multi-slider": "^0.0.1",
    "@types/react": "~16.9.0",
    "@types/react-native": "~0.60.23",
    "babel-jest": "~24.9.0",
    "jest": "^25.1.0",
    "jetifier": "~1.6.4",
    "metro-react-native-babel-preset": "~0.56.0",
    "react-test-renderer": "~16.9.0",
    "ts-jest": "^25.2.1",
    "tslint": "^6.0.0",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^4.2.0",
    "tslint-react-hooks": "^2.2.1",
    "typescript": "~3.6.3"
  }
}
